@model MVC_Project.WebBackend.Models.UserData
<script type="text/javascript">
    $(document).ready(function () {

        var columnsMetaData = [
            { data: "@Html.NameFor(x=>x.Id)", visible: false },
            { data: "@Html.NameFor(x => x.Name)", title: "@Html.DisplayNameFor(x => x.Name)" },
            { data: "@Html.NameFor(x => x.Email)", title: "@Html.DisplayNameFor(x => x.Email)" },
            { data: "@Html.NameFor(x => x.CedisName)", title: "@Html.DisplayNameFor(x => x.CedisName)" },
            { data: "@Html.NameFor(x => x.Status)", title: "@Html.DisplayNameFor(x => x.Status)" },
        ];
        var controlador = new UserIndexControlador({
            columns: columnsMetaData,
            dataTableId: "#table",
            formId: "#SearchForm"
        });
        controlador.init();
    });
</script>

